{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "resources/js/**/*", "resources/js/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/js/*"]
    },
    "types": ["vite-plugin-pwa/client"],
    "ignoreDeprecations": "5.0",
    "allowJs": true,
    "outDir": "./built"
  },

  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
